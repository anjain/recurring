<td class="text-center"><%= user.id %></td>
<td><%= user.email %></td>
<td><%= user.account.name unless user.account.blank? %></td>
<td><%= user.current_sign_in_at.strftime("%m/%d/%y") unless user.current_sign_in_at.nil? %></td>
<td><%= user.last_sign_in_ip %></td>
<td>

	<div class="dropdown">
		<button class="btn btn-default btn-xs dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
			<span class="caret"></span>
		</button>
		<ul class="dropdown-menu pull-right" aria-labelledby="dropdownMenu1">
			<li><%= link_to("View", user_path(user.id))  %></li>
			<li><%# link_to("Edit", edit_user_path(user.id)) %></li>
			<li><%# link_to("Lock", lock_order_path(user.id), :remote => true, :method => "put") %></li>
		</ul>
	</div>

</td>